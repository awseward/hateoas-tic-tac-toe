{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-be": "tsc -p src/server",
    "build": "rollup -c && tsc -p src/server",
    "build-fe": "rollup -c",
    "dev": "echo 'Prefer `watch-frontend` or `watch-backend`' && exit 1",
    "start": "node dist/",
    "watch-frontend": "rollup -c -w",
    "watch-backend": "tsc -p src/server & nodemon src/server",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/uri-templates": "^0.1.31",
    "nodemon": "^2.0.13",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "axios": "0.21.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "sirv-cli": "^1.0.0",
    "ulid": "^2.3.0",
    "uri-templates": "^0.2.0",
    "winston": "^3.3.3"
  }
}
